{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "../dist/examples",
    "module": "ESNext",
    "moduleResolution": "node",
    "types": ["node"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "declaration": false,
    "sourceMap": true
  },
  "include": [
    "*.ts"
  ],
  "references": [
    { "path": "../" }
  ]
}