# Application Environment
ENVIRONMENT=development
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# Domain Configuration
DOMAIN=localhost

# Feature Flags
ENABLE_METRICS=true
METRICS_PORT=9090
FEATURE_MEMORY_ENABLED=true
VISUALIZATION_ENABLED=true

# Performance Settings
MEMORY_LIMIT_MB=1024
AUDIO_PROCESSING_THREADS=2
FEATURE_MEMORY_CACHE_SIZE=500
FEATURE_MEMORY_TTL=3600
VISUALIZATION_QUALITY=high
PATTERN_RECOGNITION_THRESHOLD=0.65
PATTERN_LEARNING_RATE=0.1

# Error Handling
ERROR_RETRY_ATTEMPTS=5
ERROR_RETRY_DELAY=500

# API Settings
CORS_ALLOWED_ORIGINS=*
API_RATE_LIMIT=1000
API_RATE_LIMIT_WINDOW=60000

# Health Check
HEALTH_CHECK_INTERVAL=15000

# Development-specific Settings
ENABLE_HOT_RELOAD=true
ENABLE_DEBUG_LOGGING=true
ENABLE_PERFORMANCE_TRACKING=true
ENABLE_MEMORY_PROFILING=true
ENABLE_VERBOSE_ERRORS=true
DISABLE_RATE_LIMITING=true
MOCK_EXTERNAL_SERVICES=true
